SELECT m.MEMBER_NAME, r.REVIEW_TEXT, DATE_FORMAT(REVIEW_DATE, "%Y-%m-%d" ) as REVIEW_DATE
FROM REST_REVIEW r 
JOIN 
(
 SELECT a.MEMBER_NAME, a.MEMBER_ID, RANK() OVER(ORDER BY COUNT(*) DESC) AS RNK
 FROM MEMBER_PROFILE a
    JOIN REST_REVIEW b ON a.MEMBER_ID=b.MEMBER_ID
    GROUP BY a.MEMBER_NAME 
 ) m
ON m.MEMBER_ID=r.MEMBER_ID  
WHERE m.RNK=1
ORDER BY REVIEW_DATE ASC, REVIEW_TEXT ASC;
;
